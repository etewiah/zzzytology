window.Zzzytology =
  Models: {}
  Collections: {}
  Views: {}
  Routers: {}
  #init: -> alert 'Hello from Backbone!'
  init: (basepath) ->
    if  basepath == "/pages/about" 
      new Zzzytology.Routers.Contents
      Backbone.history.start({pushState: false})
    else
      new Zzzytology.Routers.Words
      Backbone.history.start({pushState: true})

